#!ipxe

:custom
clear custom_choice
menu AergernisGaming custom menu
item --gap iVentoy:
item iventoy ${space} iVentoy
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:iventoy
iseq ${platform} efi && set ipxe_disk iventoy_loader_16000_uefi && set user-class iVentoyEFI && dhcp && set user-class && chain tftp://${next-server}/iventoy_loader_16000_uefi
