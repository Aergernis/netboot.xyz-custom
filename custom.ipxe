#!ipxe

:custom
clear custom_choice
menu AergernisGaming custom menu
item --gap iVentoy:
item iventoy ${space} iVentoy
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:iventoy
iseq ${platform} efi && set user-class iVentoyEFI && dhcp && chain tftp://${next-server}/iventoy_loader_16000_uefi || set user-class iVentoyBIOS && dhcp && chain tftp://${next-server}/iventoy_loader_16000_bios
