#!ipxe

:custom
clear custom_choice
menu AergernisGaming custom menu
item --gap iVentoy:
item iventoy ${space} iVentoy
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:iventoy
iseq ${platform} efi && set user-class iVentoyEFI && dhcp && autoboot net0 net1 net2 || set user-class iVentoyBIOS && dhcp && autoboot net0 net1 net2
