#!ipxe

:custom
clear custom_choice
menu AergernisGaming custom menu
item --gap iVentoy:
item iventoy ${space} iVentoy Boot
item iventoy2 ${space} iVentoy Chain
choose custom_choice || goto custom_exit
echo ${cls}
goto ${custom_choice}
goto custom_exit

:iventoy
set user-class iventoy
dhcp
imgexec --replace tftp://${next-server}/iventoy_loader_16000_uefi

:iventoy2
set user-class iventoy
dhcp
chain --autofree tftp://${next-server}/iventoy_loader_16000_uefi

:custom_exit
exit
